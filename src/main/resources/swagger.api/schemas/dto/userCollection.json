{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "Collection of users",
  "properties": {
    "users": {
      "description": "List of users",
      "type": "array",
      "id": "userCollection",
      "items": {
        "type": "object",
        "$ref": "../response/user/user.json"
      }
    },
    "totalRecords": {
      "type": "integer"
    },
    "resultInfo": {
      "$ref": "common/resultInfo.json",
